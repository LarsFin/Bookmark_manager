<form action='signed_up' method="post">
  <p>Enter your email address</p>
  <input type='text' name='email' placeholder='person@gmail.com' required>
  <p>Enter a password</p>
  <input type='password' name='password' required>
  <p>Confirm password</p>
  <input type='password' name='confirm_password' required>
  <br> <br>
  <input type='submit' value='Sign up' method='post'>
</form>
<br>
<form action='logged_in' method="post">
  <p>Your email address</p>
  <input type='text' name='user_email' placeholder='person@gmail.com' required>
  <p>Your password</p>
  <input type='password' name='user_password' required>
  <br> <br>
  <input type='submit' value='Login' method='post'>
</form>
